{
	"datasources": {
		"defaultDataSource": {
			"ds.type": "hikari",
			"username": "dsuser",
			"password": "dspass",
			"autoCommit": true,
			"jdbcUrl": "jdbc:postgresql://localhost:5433/dsdb",
			"maximumPoolSize": 5
		},
		"coreDataSource": {
			"ds.type": "hikari",
			"username": "nucleus",
			"password": "nucleus",
			"autoCommit": false,
			"jdbcUrl": "jdbc:postgresql://localhost:5432/nucleus",
			"maximumPoolSize": 5
		}
	},
	"deps.to.deploy": [
		"org.gooru.dap.deps.ResourceConsumer",
		"org.gooru.dap.deps.CompetencyConsumer",
		"org.gooru.dap.deps.QuestionConsumer",
		"org.gooru.dap.deps.competency.AssessmentScoreEventConsumer",
		"org.gooru.dap.deps.competency.CollectionStartEventConsumer",
		"org.gooru.dap.deps.competency.CompetencyStatsEventConsumer",
		"org.gooru.dap.deps.group.GroupReportsEventConsumer"
	],
	"deps.config": {
		"org.gooru.dap.deps.competency.AssessmentScoreEventConsumer": {
			"kafka.consumer.config": {
				"instances": 1,
				"props": {
					"bootstrap.servers": "localhost:9092",
					"session.timeout.ms": 120000,
					"group.id": "group-1",
					"max.poll.records": 1,
					"max.poll.interval.ms": 1000,
					"enable.auto.commit": true
				},
				"consumer.topics": [
					"org.gooru.da.sink.gep.assessment.score"
				],
				"producer.topics": [
					"org.gooru.da.sink.dap.competency.stats",
					"org.gooru.da.sink.dap.group.reports"
				]
			}
		},
		"org.gooru.dap.deps.ResourceConsumer": {
			"kafka.consumer.config": {
				"instances": 1,
				"props": {
					"bootstrap.servers": "localhost:9092",
					"session.timeout.ms": 120000,
					"group.id": "group-2",
					"max.poll.records": 5,
					"max.poll.interval.ms": 30000,
					"enable.auto.commit": false
				},
				"consumer.topics": [
					"org.gooru.da.sink.gep.resource.timespent"
				]
			}
		},
		"org.gooru.dap.deps.QuestionConsumer": {
			"kafka.consumer.config": {
				"instances": 1,
				"props": {
					"bootstrap.servers": "localhost:9092",
					"session.timeout.ms": 120000,
					"group.id": "group-3",
					"max.poll.records": 5,
					"max.poll.interval.ms": 30000,
					"enable.auto.commit": false
				},
				"consumer.topics": [
					"org.gooru.da.sink.gep.question.timespent"
				]
			}
		},
		"org.gooru.dap.deps.competency.CollectionStartEventConsumer": {
			"kafka.consumer.config": {
				"instances": 1,
				"props": {
					"bootstrap.servers": "localhost:9092",
					"session.timeout.ms": 120000,
					"group.id": "group-4",
					"max.poll.records": 1,
					"max.poll.interval.ms": 1000,
					"enable.auto.commit": true
				},
				"consumer.topics": [
					"org.gooru.da.sink.gep.collection.start"
				]
			}
		},
		"org.gooru.dap.deps.competency.CompetencyStatsEventConsumer": {
			"kafka.consumer.config": {
				"instances": 1,
				"props": {
					"bootstrap.servers": "localhost:9092",
					"session.timeout.ms": 120000,
					"group.id": "group-5",
					"max.poll.records": 1,
					"max.poll.interval.ms": 1000,
					"enable.auto.commit": true
				},
				"consumer.topics": [
					"org.gooru.da.sink.dap.competency.stats"
				]
			}
		},
		"org.gooru.dap.deps.group.GroupReportsEventConsumer": {
			"kafka.consumer.config": {
				"instances": 1,
				"props": {
					"bootstrap.servers": "localhost:9092",
					"session.timeout.ms": 120000,
					"group.id": "group-6",
					"max.poll.records": 1,
					"max.poll.interval.ms": 1000,
					"enable.auto.commit": true
				},
				"consumer.topics": [
					"org.gooru.da.sink.dap.group.reports"
				]
			}
		}
	},
	"kafkaproducers": {
		"kafka.producer.config": {
			"instances": 1,
			"props": {
				"bootstrap.servers": "127.0.0.1:9092",
				"key.serializer": "org.apache.kafka.common.serialization.StringSerializer",
				"value.serializer": "org.apache.kafka.common.serialization.StringSerializer",
				"batch.size": 10,
				"retries": 2,
				"linger.ms": 1,
				"buffer.memory": 33554432
			},
			"producer.topics": [
				"org.gooru.da.sink.dap.competency.stats"
			]
		}
	},
	"systemProperties": {
		"logback.configurationFile": "src/main/resources/logback-dap.xml"
	},
	"comment": "cron expr (0 0 6 * * ?) runs every day 6 am. for dev testing use 0 */2 * ? * * (runs every 2 min)",
	"jobs.config": {
		"jobChain": [
			{
				"chainId": "org.gooru.dap.jobs.schedular.init.JobChainOne",
				"config": {
					"cronExpression": "0 */1 * ? * *",
					"jobConfigs": [
						{
							"jobId": "org.gooru.dap.jobs.GroupReportsDataAggregatorExecutor"
						}
					]
				}
			}
		]
	}
}
