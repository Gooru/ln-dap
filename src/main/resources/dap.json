{
  "datasources": {
    "defaultDataSource": {
      "ds.type": "hikari",
      "username": "dsuser",
      "password": "dspass",
      "autoCommit": true,
      "jdbcUrl": "jdbc:postgresql://localhost:5433/dsdb",
      "maximumPoolSize": 5
    },
    "coreDataSource": {
      "ds.type": "hikari",
      "username": "nucleus",
      "password": "nucleus",
      "autoCommit": false,
      "jdbcUrl": "jdbc:postgresql://localhost:5432/nucleus",
      "maximumPoolSize": 5
